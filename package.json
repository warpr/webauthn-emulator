    "*.js": [
      "prettier --write"
    ],
    "*.mjs": [
      "prettier --write"
    ]
  },
  "author": "Kuno Woudt <kuno@frob.nl>",
  "license": "MIT",
  "repository": "https://github.com/warpr/webauthn-emulator.git",
  "devDependencies": {
    "@prettier/plugin-php": "^0.21.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.0.3"
  },
  "scripts": {
    "postinstall": "git config --local core.hooksPath etc/hooks"
  }
}
